<div class="container login">
  <div class="container">
    <div class="row">
      <div class="col-sm-6 col-sm-offset-3">
      <h3>Add a review</h3>
        <%= simple_form_for [@booking, @review] do |f| %>
        <%= f.input :rating, collection: 1..5, as: :select %>
        <%= f.input :content %>
        <%= f.submit class: "btn btn-primary"%>
      <% end %>
      </div>
    </div>
  </div>
</div>

